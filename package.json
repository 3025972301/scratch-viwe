{
  "name": "scratch-viwe",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "deploy": "npm run build && wrangler deploy",
    "deploy:preview": "npm run build && wrangler deploy --env preview",
    "cf:login": "wrangler login",
    "cf:d1:create": "wrangler d1 create scratch-viwe-db",
    "cf:d1:init": "wrangler d1 execute scratch-viwe-db --file=./schema.sql",
    "cf:d1:init:local": "wrangler d1 execute scratch-viwe-db --local --file=./schema.sql",
    "cf:r2:create": "wrangler r2 bucket create scratch-viwe-uploads",
    "cf:dev": "npm run build && wrangler dev"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.7",
    "@mdi/font": "7.4.47",
    "hono": "^4.6.0",
    "jszip": "^3.10.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.35.0",
    "eslint-config-vuetify": "^4.2.0",
    "sass-embedded": "^1.92.1",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.5",
    "vite-plugin-vuetify": "^2.1.2",
    "wrangler": "^4.54.0"
  }
}
